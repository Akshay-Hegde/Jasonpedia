{
  "$jason": {
    "head": {
      "title": "Jasonette => Web Container $agent function call",
      "description": "This example takes user input from native body.footer.input component and sends it to the web container, calling the qr code update function",
      "actions": {
        "$load": {
          "type": "$render",
          "success": {
            "type": "$agent.inject",
            "options": {
              "id": "$webcontainer",
              "items": [
                {
                  "url": "https://cdnjs.cloudflare.com/ajax/libs/pulltorefreshjs/0.1.7/pulltorefresh.js"
                },
                {
                  "url": "https://jasonette.github.io/Jasonpedia/webcontainer/agent/inject/injection.js"
                }
              ]
            }
          }
        }
      },
      "templates": {
        "body": {
          "header": {
            "title": "Pull to refresh injection"
          },
          "background": {
            "type": "html",
            "url": "https://coinmarketcap.com",
            "action": {
              "type": "$default"
            }
          }
        }
      }
    }
  }
}
